{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "REACT_APP_TELEGRAM_BOT_TOKEN": "@react_app_telegram_bot_token",
    "REACT_APP_API_BASE_URL": "@react_app_api_base_url"
  }
}